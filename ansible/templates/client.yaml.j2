server:
  url: "{{ s3uploader_server_url }}"
  api_key: "{{ client_api_key }}"

database:
  path: "{{ client_dir }}/client.db"

watches:
{% for watch in client_watches %}
  - local_path: "{{ watch.local_path }}"
    remote_prefix: "{{ watch.remote_prefix | default('') }}"
{% endfor %}

scan:
  upload_existing: true
